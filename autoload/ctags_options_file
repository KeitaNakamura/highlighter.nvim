--langdef=julia
--langmap=julia:.jl
--regex-julia=/^[ \t]*(function)[ \t]+([^ \t({[]+).*$/\2/f,function/
--regex-julia=/^[ \t]*(macro)[ \t]+([^ \t({[]+).*$/\2/m,macro/
--regex-julia=/^[ \t]*(abstract|type|typealias|immutable)[ \t]+([^ \t({[]+).*$/\2/t,type/
--regex-julia=/^[ \t]*(([^@#$ \t({[]+)|\(([^@#$ \t({[]+)\)|\((\$)\))[ \t]*(\{.*\})?[ \t]*\([^#]*\)[ \t]*=([^=].*$|$)/\2\3\4/f,function/
